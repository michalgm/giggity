    "@feathersjs/socketio": "^3.2.7",
    "@feathersjs/socketio-client": "^1.2.1",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "auth0": "^2.18.0",
    "aws-sdk": "^2.501.0",
    "axios": "^0.18.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "email-templates": "^5.1.0",
