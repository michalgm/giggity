{
  "name": "giggity",
  "description": "Giggity Backend",
  "version": "0.0.0",
  "homepage": "",
  "main": "./backend",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Greg Michalec",
    "email": "greg@primate.net"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "./backend",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "start-client": "PORT=4000 BROWSER=none react-scripts start",
    "start-server": "NODE_ENV=development PORT=3030 nodemon ./backend/",
    "start:dev": "concurrently -k \"yarn run start-server\" \"yarn run start-client\"",
    "test-backend": "yarn run eslint && NODE_ENV= yarn run mocha",
    "eslint": "eslint ./backend/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "build": "react-scripts build",
    "gcp-build": "react-scripts build",
    "start": "NODE_ENV=production node backend/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy-quick": "ssh giggity -C \"cd giggity-dev; git pull && npm run build && sudo systemctl restart giggity.service\"",
    "deploy": "ssh giggity -C \"cd giggity-dev; git pull && npm ci && npm run build && sudo systemctl restart giggity.service\""
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.13",
